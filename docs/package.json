{
  "name": "deepdive",
  "version": "9.1.7",
  "description": "Why DeepDive ðŸŒŠ Because the format is liquid ðŸ’¦ ðŸ˜‰",
  "scripts": {
    "start": "bundle exec jekyll serve -l -w --trace --safe",
    "build": "bundle exec jekyll build",
    "clean": "rm -vf ./assets/js{,/legacy}/*{.map,.min.js,.LICENSE.txt}",
    "test": "exit 0",
    "format": "prettier --write '_js/src/**/*.js'",
    "build:yarn": "yarn run clean && yarn run build:js & yarn run build:css & yarn run build:scripts & wait",
    "build:js": "webpack --mode production --color",
    "build:js:stats": "webpack --mode production --progress --color --json > stats.json",
    "build:css": ".scripts/build-css.js",
    "build:scripts": "./.bash/osx/uglifyjs.sh",
    "watch": "yarn run clean && yarn run watch:js & yarn run watch:css",
    "watch:js": "webpack --watch --mode development --progress --color",
    "watch:css": "yarn run build:css && onchange '_sass/**/*.scss' -e '_sass/**/__*/*' -- yarn run build:css '{{changed}}'",
    "dev": "yarn run watch",
    "preversion": "yarn run preversion:js && yarn run preversion:css",
    "preversion:js": "sed -i '' 's:/assets/js/\\*deepdive-\\*:#<removed/js>:' .gitignore",
    "preversion:css": "sed -i '' 's:/_sass/\\*/__\\*__/\\*:#<removed/css>:' .gitignore",
    "version": ".scripts/version.js && yarn run format && yarn run build && git add .",
    "postversion": "(yarn run postversion:js && yarn run postversion:css) && git add . && git ci -m 'Restore pre-version .gitignore' && yarn run postversion:forward",
    "postversion:forward": "git branch -f v9 v$npm_package_version",
    "postversion:js": "sed -i '' 's:#<removed/js>:/assets/js/*deepdive-*:' .gitignore && git rm --cached assets/js/*deepdive-*",
    "postversion:css": "sed -i '' 's:#<removed/css>:/_sass/*/__*__/*:' .gitignore && git rm --cached _sass/*/__*__/*"
  },
  "config": {
    "port": 4000
  },
  "author": "JV conseil <contact@jv-conseil.net> (https://www.jv-conseil.net)",
  "license": "EUPL-1.2",
  "private": true,
  "dependencies": {
    "@fontsource/noto-sans": "^5.0.22",
    "@fontsource/roboto-slab": "^5.0.20",
    "@hydecorp/component": "^1.0.0",
    "@hydecorp/drawer": "^1.0.0",
    "@hydecorp/push-state": "^1.0.0",
    "@juggle/resize-observer": "^3.4.0",
    "@popperjs/core": "^2.11.8",
    "@webcomponents/custom-elements": "^1.6.0",
    "@webcomponents/shadycss": "^1.11.2",
    "@webcomponents/shadydom": "^1.11.0",
    "@webcomponents/template": "^1.5.1",
    "@webcomponents/url": "^0.7.9",
    "@webcomponents/webcomponents-platform": "^1.0.1",
    "abortcontroller-polyfill": "^1.7.5",
    "bootstrap": "^5.3.3",
    "broadcastchannel-polyfill": "^1.0.1",
    "core-js": "^3.37.1",
    "create-element-x": "^1.2.0",
    "dompurify": "^3.1.5",
    "intersection-observer": "^0.12.2",
    "jquery": "^3.7.1",
    "kv-storage-polyfill": "^2.0.0",
    "lit": "^3.1.4",
    "minisearch": "^6.3.0",
    "rxjs": "^7.8.1",
    "scroll-into-view-if-needed": "^3.1.0",
    "tippy.js": "^6.3.7",
    "web-animations-js": "^2.3.2",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-loader": "^9.1.3",
    "bower": "^1.8.14",
    "braces": "^3.0.3",
    "dedent": "^1.5.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "null-loader": "^4.0.1",
    "onchange": "^7.1.0",
    "prettier": "^3.3.2",
    "tar": "^7.4.0",
    "uglify-js": "^3.18.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0",
    "yargs": "^17.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JV-conseil/jekyll-theme-deepdive.git"
  },
  "bugs": {
    "url": "https://github.com/JV-conseil/jekyll-theme-deepdive/issues"
  },
  "homepage": "https://www.jv-conseil.net/",
  "engines": {
    "node": ">=11"
  },
  "packageManager": "yarn@4.3.1"
}
